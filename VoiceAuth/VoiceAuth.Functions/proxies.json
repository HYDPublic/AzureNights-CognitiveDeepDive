{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "index": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://localhost/api/static/login.html"
    },
    "static": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/{route}"
      },
      "backendUri": "https://localhost/api/static/{route}"
    }
  }
}